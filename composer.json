{
	"name": "laraditz/repository",
	"description": "Repository implementation for Laravel.",
	"type": "laravel-package",
	"license": "MIT",
	"authors": [
		{
			"name": "Raditz Farhan",
			"email": "raditzfarhan@gmail.com"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": "^7.2.5",
		"illuminate/support": "^7.0|^8.0",
		"illuminate/console": "^7.0|^8.0"
	},
	"autoload": {
		"psr-4": {
			"Laraditz\\Repository\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [ "Laraditz\\Repository\\RepositoryServiceProvider" ],
			"aliases": {
				"Repository": "Laraditz\\Repository\\Facades\\Repository"
			}
		}
	}
}
